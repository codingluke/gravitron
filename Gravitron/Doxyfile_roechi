# Die Dokumentation befindet sich im Unterverzeichnis doc.
OUTPUT_DIRECTORY = doc
# Basis für die Dokumentation ist das aktuelle Verzeichnis.
INPUT = . ./src/ ./qml/ ./src/headers/
# Alle in Frage kommenden Dateien werden untersucht.
EXTRACT_ALL = YES
# Das Werkzeug dot wird zum Zeichnen der Aufrufhierarchie benutzt. # Es gehört zur Visualisierungssoftware Graphviz.
# Siehe: http://www.graphviz.org/
HAVE_DOT = YES
# Die Aufrufhierarchie wird erstellt.
CALL_GRAPH = YES

